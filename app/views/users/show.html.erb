<h1>User info</h1>
<div>
  <h3><%= @user.name %></h3>
  <%= image_tag @user.get_profile_image(100,100) %>
  <% if @user == current_user.id %>
    <p><%= link_to "プロフィール編集", @user %></p>
  <% end %>
</div>


<% @books.each do |book| %>
<div>
  <%= link_to books_path(book.id) do %>
  <% end %>
  <p><%= image_tag book.user.get_profile_image(100,100) %></p>
  <p>title：<%= book.title %></p>
  <p>Option：<%= book.body %></p>
  <p>ユーザーネーム：<%= book.user.name %></p>
</div>
<% end %>