<h1>Books</h1>

<table class="table">
  <thead>
    <th></th>
    <th>Title</th>
    <th>Option</th>
  </thead>
  <tbody>
    <% @books.each do |book| %>
    <tr>
    <td><%= image_tag book.user.get_profile_image(100,100) %></td>
      <td><%= book.title %></td>
      <td><%= book.body %></td>
    </tr>
    <% end %>
  </tbody>
</table>